import RichTapUtils from 'com.apprichtap.haptic.RichTapUtils'
import PrebakedEffectId from 'com.apprichtap.haptic.base.PrebakedEffectId'
import { PLUGIN_VERSION } from '../define.uts'
import { HapticError } from '../unierror.uts'
import { GetSDKVersion, GetPluginVersion, IsRichTapSupported, Initialize, Quit, PlayHaptic, StopHaptic, UpdateLoopParameters, PlayExtPrebaked, PrebakedEffectName } from '../interface.uts'

/**
 * 获取SDK版本号
 * @param success 成功回调 返回SDK版本号
 * @param fail 错误回调 可选参数 当发送异常时会通过fail回调函数把错误抛出
 */
export const getSDKVersion: GetSDKVersion = (options): void => {
	try{
		options.success(RichTapUtils.VERSION_NAME)
	} catch (err){
		console.error(err)
		options.fail?.(new HapticError(9010001, new Error(err.toString())))
	}
}

/**
 * 获取当前插件版本
 * @param success 成功回调 返回插件版本号
 * @param fail 错误回调 可选参数 当发送异常时会通过fail回调函数把错误抛出
 */
export const getPluginVersion: GetPluginVersion = (options): void => {
	try{
		options.success(PLUGIN_VERSION)
	} catch (err){
		console.error(err)
		options.fail?.(new HapticError(9010002, new Error(err.toString())))
	}
}

/**
 * 判断是否支持RichTap振动
 * @param success 成功回调 返回布尔值代表是否支持高品质振动
 * @param fail 错误回调 可选参数 当发送异常时会通过fail回调函数把错误抛出
 */
export const isRichTapSupported: IsRichTapSupported = (options): void => {
	try{
		options.success(RichTapUtils.getInstance().isSupportedRichTap())
	} catch (err) {
		console.error(err)
		options.fail?.(new HapticError(9010003, new Error(err.toString())))
	}
}

/**
 * 初始化RichTap插件
 * @param success 成功回调 可选参数
 * @param fail 错误回调 可选参数 当发送异常时会通过fail回调函数把错误抛出
 */
export const initialize: Initialize = (options): void => {
	try{
		RichTapUtils.getInstance().init(UTSAndroid.getAppContext())
		options.success?.()
	} catch (err){
		console.error(err)
		options.fail?.(new HapticError(9010004, new Error(err.toString())))
	}
}

/**
 * 释放/退出RichTap资源
 * @param success 成功回调 可选参数
 * @param fail 错误回调 可选参数 当发送异常时会通过fail回调函数把错误抛出
 */
export const quit: Quit = (options): void => {
	try{
		RichTapUtils.getInstance().quit()
		options.success?.()
	} catch (err){
		console.error(err)
		options.fail?.(new HapticError(9010005, new Error(err.toString())))
	}
}

/**
 * 播放振动效果
 * @param heStr json字符串
 * @param loop 循环次数 取值范围是大于等于-1，-1表示无限再循环
 * @param interval 间隔 表示两次播放之间的时间间隔
 * @param intensity 强度缩放值 取值范围是[0, 511]，255表示不对HE强度做缩放
 * @param frequency 变频值 取值范围[-100, 100]，0表示不对HE做变频
 * @param success 成功回调 可选参数
 * @param fail 错误回调 可选参数 当发送异常时会通过fail回调函数把错误抛出
 */
export const playHaptic: PlayHaptic = (options): void => {
	try {
		RichTapUtils.getInstance().playHaptic(options.heStr, options.loop.toInt(), options.interval.toInt(), options.intensity.toInt(), options.frequency.toInt())
		options.success?.()
	} catch (err) {
		console.error(err)
		options.fail?.(new HapticError(9010006, new Error(err.toString())))
	}
}

/**
 * 停止所有振动
 * @param success 成功回调 可选参数
 * @param fail 错误回调 可选参数 当发送异常时会通过fail回调函数把错误抛出
 */
export const stopHaptic: StopHaptic = (options): void => {
	try {
		RichTapUtils.getInstance().stop()
		options.success?.()
	} catch (err) {
		console.error(err)
		options.fail?.(new HapticError(9010007, new Error(err.toString())))
	}
}

/**
 * 设置播放效果参数
 * @param intensity 强度缩放值 取值范围是[0, 255]，255表示不对HE强度做缩放
 * @param interval 间隔 表示两次播放之间的时间间隔
 * @param frequency 变频值 取值范围[-100, 100]，0表示不对HE做变频
 * @param success 成功回调 可选参数
 * @param fail 错误回调 可选参数 当发送异常时会通过fail回调函数把错误抛出
 */
export const updateLoopParameters: UpdateLoopParameters = (options): void => {
	try {
		RichTapUtils.getInstance().sendLoopParameter(options.intensity.toInt(), options.interval.toInt(), options.frequency.toInt())
		options.success?.()
	} catch (err) {
		console.error(err)
		options.fail?.(new HapticError(9010008, new Error(err.toString())))
	}
}

